@model List<WebApplication1.Models.Agent>

@{
    ViewBag.Title = "Index";
}

<h2>@ViewData["subTitle"]</h2>

<h3>@ViewData["status"]</h3>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Agent Name
        </th>
        <th>
            Agent Contact No.
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mob)
        </td>
        <td>
       
            @Html.ActionLink("Edit", "Edit", "Agent", new { id=item.id },null) |
            @Html.ActionLink("Details", "Details", "Agent", new { id=item.id }, null) |
            @Html.ActionLink("Delete", "Delete", "Agent", new { id=item.id }, FormMethod.Post)
        </td>
    </tr>
}

</table>
